{
  "info": {
    "name": "NutriMind API",
    "description": "Complete API collection for NutriMind meal planning application",
    "schema": "https://schema.getpostman.com/json/collection/v2.1.0/collection.json"
  },
  "variable": [
    {
      "key": "baseUrl",
      "value": "http://localhost:7071",
      "description": "Base URL for the API"
    },
    {
      "key": "authToken",
      "value": "your-jwt-token-here",
      "description": "JWT authentication token"
    }
  ],
  "item": [
    {
      "name": "Profile API",
      "item": [
        {
          "name": "Create User Profile",
          "request": {
            "method": "POST",
            "header": [
              {
                "key": "Content-Type",
                "value": "application/json"
              }
            ],
            "body": {
              "mode": "raw",
              "raw": "{\n  \"email\": \"john.doe@example.com\",\n  \"firstName\": \"John\",\n  \"lastName\": \"Doe\",\n  \"age\": 30,\n  \"height\": 175.5,\n  \"weight\": 70.0,\n  \"activityLevel\": \"moderate\",\n  \"dietaryPreference\": \"vegetarian\",\n  \"allergens\": [\"nuts\", \"shellfish\"],\n  \"dislikes\": [\"mushrooms\", \"cilantro\"],\n  \"healthGoals\": [\"weight_loss\", \"muscle_gain\"],\n  \"targetCalories\": 2000,\n  \"mealFrequency\": 3,\n  \"cookingSkillLevel\": \"intermediate\",\n  \"cookingTimePreference\": 45,\n  \"budgetPerWeek\": 75.00,\n  \"preferredCuisines\": [\"mediterranean\", \"asian\", \"italian\"]\n}"
            },
            "url": {
              "raw": "{{baseUrl}}/api/profile",
              "host": ["{{baseUrl}}"],
              "path": ["api", "profile"]
            }
          }
        },
        {
          "name": "Get User Profile",
          "request": {
            "method": "GET",
            "header": [
              {
                "key": "Authorization",
                "value": "Bearer {{authToken}}"
              }
            ],
            "url": {
              "raw": "{{baseUrl}}/api/profile",
              "host": ["{{baseUrl}}"],
              "path": ["api", "profile"]
            }
          }
        },
        {
          "name": "Update User Profile",
          "request": {
            "method": "PUT",
            "header": [
              {
                "key": "Authorization",
                "value": "Bearer {{authToken}}"
              },
              {
                "key": "Content-Type",
                "value": "application/json"
              }
            ],
            "body": {
              "mode": "raw",
              "raw": "{\n  \"targetCalories\": 2200,\n  \"cookingSkillLevel\": \"advanced\",\n  \"budgetPerWeek\": 85.00\n}"
            },
            "url": {
              "raw": "{{baseUrl}}/api/profile",
              "host": ["{{baseUrl}}"],
              "path": ["api", "profile"]
            }
          }
        },
        {
          "name": "Delete User Profile",
          "request": {
            "method": "DELETE",
            "header": [
              {
                "key": "Authorization",
                "value": "Bearer {{authToken}}"
              }
            ],
            "url": {
              "raw": "{{baseUrl}}/api/profile",
              "host": ["{{baseUrl}}"],
              "path": ["api", "profile"]
            }
          }
        }
      ]
    },
